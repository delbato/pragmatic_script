import std::println;
import std::printi;
import std::print;

fn: main() ~ int {
    var hello: string = "Hello from PragmaticScript!";
    var i: int = 0;
    while i < 1000 {
        var tmp: int = printi(i);
        tmp = println("");
        //blub = printi(tmp);
        //blub = println("");
        tmp = println(hello);
        i = i + 1;
    }
    return i;
}

